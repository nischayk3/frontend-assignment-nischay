{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015"],
     "jsx": "react-jsx",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "noImplicitAny": true,
    //   "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }